{
  "compilerOptions": {
    "target": "ESNext",
    "outDir": "./dist",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "CommonJS",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "babel.config.js"  ,
"src/**/*", "prisma/seed.ts"

],
  "exclude": [
    "node_modules"
  ]
}
